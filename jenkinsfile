pipeline {
    agent any
    stages{
        stage ('Checkout'){
            steps {
                git branch: 'main', credentialsId: 'Akash@1994', url: 'https://github.com/skasbp/AkashGithub2025.git'
                sh 'echo "Welcome t0 my world"'
            }
        }
        stage ('Build'){
            steps {
                sh '''
                    pwd
                    ls -lrt
                '''
            }
        }
        stage ('Stages3') {
            agent { label 'SLAVE 2'}
            steps {
                sh 'cat VCSfile.txt'
            }
        }
    }
}
